#include <iostream>
#include <sstream>
#include <string>
using namespace std;

void input() {
	string tmp;
	
	while (true) {
		cout << "Input please" << endl;
		getline(cin, tmp);
		if (tmp.empty()) break;
		cout << "First: " << tmp[0] << endl;
		cout << "Second: " << tmp[2] << endl;
	}
}

int main() {
	input();

	return 1;
}